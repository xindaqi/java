<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.anotherdatasource.dao.DeviceStatusDao">
    <!-- 查询设备信息 -->
    <select id="selectDeviceStatusById" resultType="com.anotherdatasource.po.DeviceStatus" parameterType="Integer">
        select * from device_status where id=#{id}
    </select>
</mapper>
